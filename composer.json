{
    "name": "cypresslab/walrus",
    "autoload": {
        "psr-0": {
            "Walrus": [
                "src/",
                "tests/"
            ]
        }
    },
	"require": {
        "php": "5.4.*",
		"symfony/console": "2.1.*@stable",
		"symfony/config": "2.1.*@stable",
		"symfony/dependency-injection": "2.1.*@stable",
		"symfony/yaml": "2.1.*@stable",
		"symfony/finder": "2.1.*@stable",
		"symfony/filesystem": "2.1.*@stable",
		"twig/twig": "1.10.*@stable",
		"dflydev/markdown": "1.0.*",
        "cypresslab/less-elephant": "0.2.*@stable",
        "cypresslab/compass-elephant": "0.2.*@stable",
        "kriswallsmith/assetic": "dev-master",
        "kriswallsmith/spork": "dev-master",
        "nervo/yuicompressor": "@stable",
        "dflydev/github-gist-twig-extension": "@stable"
	},
	"require-dev": {
	    "phpunit/phpunit": "3.7.*"
	},
	"bin": ["bin/walrus"],
	"minimum-stability": "dev"
}
